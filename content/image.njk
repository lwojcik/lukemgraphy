---
layout: page
pagination:
    data: galleryData.images
    size: 1
    alias: image
    addAllPagesToCollections: true
permalink: "{{ image.link }}"
eleventyComputed:
  title: "{{ image.name }} - {{ image.parent.gallery.name }}"
---

